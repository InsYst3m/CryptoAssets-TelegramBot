{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "${{ secrets.CONNECTION_STRING }}"
  },
  "BotSettings": {
    "Token": "${{ secrets.BOT_TOKEN }}"
  },
  "NotificationsSettings": {
    "StartHourUTC": 3,
    "EndHourUTC": 19,
    "ChatId": "${{ secrets.CHAT_ID }}"
  },
  "TimerWrapperSettings": {
    "PeriodicTimerIntervalInMinutes": 180
  }
}
